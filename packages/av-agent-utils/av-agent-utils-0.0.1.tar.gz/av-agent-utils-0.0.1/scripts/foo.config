  "schedule": {
    "asset_heartbeat": {
      "description": "A heartbeat that reports information needed to identify the asset.", 
      "interval": 120, 
      "platform": "windows", 
      "on_startup": true, 
      "query": "SELECT hostname, computer_name, local_hostname, case instr(osquery_info.version, '_') when 0 then '1.0.1' else substr(osquery_info.version, instr(osquery_info.version, '_')+1) end as version, os_version.name as os_name, os_version.version as os_version, platform as build_platform, unix_time, group_concat(interface) as interface, group_concat(mac) as mac, group_concat(type) as type, group_concat(dns_domain) as dns_domain, group_concat(dns_host_name) as dns_host_name, group_concat(address) as address FROM system_info, osquery_info, os_version, time, (SELECT group_concat(interface_details.interface) as interface, group_concat(interface_details.mac) as mac, group_concat(interface_details.type) as type, group_concat(interface_details.dns_domain) as dns_domain, group_concat(interface_details.dns_host_name) as dns_host_name, group_concat(interface_addresses.address) as address FROM interface_details INNER JOIN interface_addresses ON interface_details.interface = interface_addresses.interface WHERE (interface_details.mac NOT LIKE '00:00:00:00:00:%' AND interface_details.mac != '' AND interface_addresses.address NOT LIKE '%:%') GROUP BY interface_details.interface);", 
      "removed": false
    }
  }, 
