MODULES=''
SERVER=''
CHANNEL=''
NICK=''
OWNER='`

botd -x -w /var/lib/botd cfg krn modules ${1-MODULES}
botd -x -w /var/lib/botd cfg irc server ${2-SERVER}
botd -x -w /var/lib/botd cfg irc channel ${3-CHANNEL}
botd -x -w /var/lib/botd cfg irc nick ${4-NICK}
botd -x -w /var/lib/botd meet ${5-OWNER}
botd -x -w /var/lib/botd cfg krn nocmds true
botd -x -w /var/lib/botd cfg krn

