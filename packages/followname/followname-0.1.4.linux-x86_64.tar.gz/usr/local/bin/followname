#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'followname==0.1.4','console_scripts','followname'
__requires__ = 'followname==0.1.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('followname==0.1.4', 'console_scripts', 'followname')()
    )
