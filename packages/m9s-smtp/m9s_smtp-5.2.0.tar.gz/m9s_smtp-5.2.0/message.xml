<tryton>
    <data group="1">
      <record model="ir.message" id="smtp_successful">
          <field name="text">SMTP Test connection was successful</field>
      </record>
      <record model="ir.message" id="smtp_test_details">
          <field name="text">SMTP Test connection details:
%(error)s</field>
      </record>
      <record model="ir.message" id="smtp_error">
          <field name="text">SMTP Test Connection Failed.</field>
      </record>
      <record model="ir.message" id="server_model_not_found">
          <field name="text">There are not SMTP server related at model %(model)s</field>
      </record>
      <record model="ir.message" id="smtp_exception">
          <field name="text">SMTP Server exception (Email have not been sent):
%(error)s</field>
      </record>
      <record model="ir.message" id="smtp_server_error">
          <field name="text">Error: could not connect to server. (Email have not been sent)</field>
      </record>
    </data>
</tryton>
