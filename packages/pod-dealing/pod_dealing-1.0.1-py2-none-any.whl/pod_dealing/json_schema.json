      "agentLastName": {
        "type": "string",
        "minLength": 1,
        "title": "نام خانوادگی نماینده"
      },
      "agentCellphoneNumber": {
        "type": "string",
        "pattern": "^(0|\\+98|0098){1}[9]{1}[\\d]{9}$",
