;
; Configuration file for coverage tool.
;
[run]
branch = False
data_file = /tmp/.coverage
parallel = True
source = aakbar
omit =
    examples/*
    test/*
    version.py

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
