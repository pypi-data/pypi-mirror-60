<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.sendinblue"
    purge="False">

    <configlet
        title="Sendinblue"
        action_id="sendinblue"
        appId="collective.sendinblue"
        category="Products"
        condition_expr=""
        icon_expr="string:${portal_url}/++resource++collective.sendinblue.images/sendinblue.png"
        url_expr="string:${portal_url}/@@sendinblue-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
